{
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "overrides": [
      {
        "files": ["*.js"],
        "rules": {
          // You can override or disable rules here
          // For example, to turn off the no-unused-vars rule, uncomment the next line
          // "no-unused-vars": "off"
        }
      }
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    }
  }
  